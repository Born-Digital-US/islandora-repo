        </svg>
      </a>
      <a href="/search?f[0]=open_access%3A1">{{ item.content }}</a>
    </div>
  {% endfor %}
{% else %}
  <div{{ attributes.addClass(classes) }}>
    <div{{ title_attributes.addClass(title_classes) }}>{{ label }}</div>
    {% for item in items %}
      <div{{ attributes.addClass(classes, 'field__item') }}>
        <a class="icon-link" href="/search?f[0]=open_access%3A1">
          <svg id="open-access_cache126" width="100%" height="100%" viewBox="0 0 24 24" y="1634" xmlns="http://www.w3.org/2000/svg" fit="" preserveAspectRatio="xMidYMid meet" focusable="false">
